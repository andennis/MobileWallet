@model PassContentFieldViewModel

@Html.HiddenForExt(m => m.PassContentFieldId)

@if (Model.FieldKind.HasValue)
{
    @Html.TextBlockFormForExt(m => m.FieldKind, "Location")
}

@Html.TextBlockFormForExt(m => m.FieldName, "Name")

@if (Model.FieldKind.HasValue)
{
    @Html.TextBoxFormForExt(m => m.FieldLabel, "Label")
}

@Html.TextBoxFormForExt(m => m.FieldValue, "Value")