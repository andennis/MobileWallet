@{
    Layout = "~/Views/Shared/_LayoutDetails.cshtml";
}

@section Title
{
    Create @Model.DisplayName
}

@RenderBody()

@section Actions
{
    <button type="submit" class="btn btn-default btn-primary">Create</button>
    @Html.ActionLinkExt("Cancel", "Index", htmlAttributes: new { @class = "btn btn-default" })
}