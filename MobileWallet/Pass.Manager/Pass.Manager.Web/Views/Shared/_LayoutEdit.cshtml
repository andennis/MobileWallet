@{
    Layout = "~/Views/Shared/_LayoutDetails.cshtml";
}

@section Title
{
    Edit @Model.DisplayName
}

@RenderBody()

@section Actions
{
    <button type="submit" class="btn btn-default btn-primary">Save</button>
    @Html.ActionLinkExt("Cancel", "Index", htmlAttributes: new {@class = "btn btn-default"})
}
